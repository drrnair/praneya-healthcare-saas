{
  "name": "praneya-healthcare-saas",
  "version": "1.0.0",
  "description": "Healthcare nutrition SaaS platform with clinical AI integration",
  "main": "dist/server/index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:healthcare-mode": "cross-env NODE_ENV=development HEALTHCARE_MODE=true npm run dev",
    "build": "npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "lint": "next lint && eslint \"src/server/**/*.ts\"",
    "lint:healthcare": "eslint \"src/**/*.ts\" --config .eslintrc.healthcare.js",
    "type-check": "tsc --noEmit && tsc -p tsconfig.server.json --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:healthcare-compliance": "jest --testPathPattern=tests/healthcare --verbose",
    "test:clinical-safety": "jest --testPathPattern=tests/clinical",
    "test:family-privacy": "jest --testPathPattern=tests/family",
    "test:hipaa-compliance": "jest --testPathPattern=tests/hipaa",
    "test:api-integration": "jest --testPathPattern=tests/api",
    "test:performance-healthcare": "jest --testPathPattern=tests/performance",
    "test:security-penetration": "npm run test:security",
    "test:security": "jest --testPathPattern=tests/security",
    "test:db:connection": "jest --config jest.database.config.js --testPathPattern=tests/database/connection",
    "test:db:security": "jest --config jest.database.config.js --testPathPattern=tests/database/security",
    "test:healthcare:compliance": "jest --config jest.database.config.js --testPathPattern=tests/healthcare --verbose",
    "test:tenant:isolation": "jest --config jest.database.config.js --testPathPattern=tests/tenant",
    "test:clinical:drug-interactions": "jest --config jest.database.config.js --testPathPattern=tests/clinical/drug-interactions",
    "test:clinical:allergy-screening": "jest --config jest.database.config.js --testPathPattern=tests/clinical/allergy-screening",
    "test:healthcare:all": "jest --config jest.database.config.js --testPathPattern=tests/healthcare --testPathPattern=tests/clinical --testPathPattern=tests/tenant --testPathPattern=tests/database --verbose",
    "test:clinical:safety": "jest --config jest.database.config.js --testPathPattern=tests/clinical",
    "test:clinical:allergen-detection": "jest --config jest.database.config.js --testPathPattern=tests/clinical/allergen-detection",
    "test:hipaa:compliance": "jest --config jest.database.config.js --testPathPattern=tests/hipaa/compliance",
    "test:hipaa:audit-trails": "jest --config jest.database.config.js --testPathPattern=tests/hipaa/audit-trails",
    "test:hipaa:data-encryption": "jest --config jest.database.config.js --testPathPattern=tests/hipaa/data-encryption",
    "test:family:privacy": "jest --config jest.database.config.js --testPathPattern=tests/family/privacy",
    "test:family:permissions": "jest --config jest.database.config.js --testPathPattern=tests/family/permissions",
    "test:family:emergency-access": "jest --config jest.database.config.js --testPathPattern=tests/family/emergency-access",
    "test:security:tenant-isolation": "jest --config jest.database.config.js --testPathPattern=tests/security/tenant-isolation",
    "test:security:rbac": "jest --config jest.database.config.js --testPathPattern=tests/security/rbac",
    "test:security:device-fingerprinting": "jest --config jest.database.config.js --testPathPattern=tests/security/device-fingerprinting",
    "test:security:encryption": "jest --config jest.database.config.js --testPathPattern=tests/security/encryption",
    "test:security:audit-logging": "jest --config jest.database.config.js --testPathPattern=tests/security/audit-logging",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx database/scripts/seed.ts",
    "db:reset": "tsx database/scripts/reset.ts",
    "health-data:rollback": "tsx scripts/health-data-rollback.ts",
    "clinical:validate": "tsx scripts/clinical-validation.ts",
    "hipaa:audit": "tsx scripts/hipaa-audit.ts",
    "monitor:api-costs": "tsx scripts/monitor-api-costs.ts",
    "optimize:query-cache": "tsx scripts/optimize-cache.ts",
    "emergency:safety-incident": "tsx scripts/emergency-response.ts",
    "docker:build": "docker build -f docker/Dockerfile .",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker/docker-compose.prod.yml up"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
