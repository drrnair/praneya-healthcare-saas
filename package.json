    "@types/d3": "^7.4.3",
    "@vercel/analytics": "^1.1.1",
    "argon2": "^0.31.2",
    "axios": "^1.10.0",
    "axios-rate-limit": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
